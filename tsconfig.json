{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "~/*": ["src/*"],
      "@mcneilweb/sudoku": ["node_modules/@mcneilweb/sudoku/dist/index.*"],
      "@mcneilweb/sudoku/*": ["node_modules/@mcneilweb/sudoku/dist/*"],
      "@mcneilweb/ui": ["node_modules/@mcneilweb/ui/dist/index.*"],
      "@mcneilweb/ui/*": ["node_modules/@mcneilweb/ui/dist/*"]
    }
  }
}
